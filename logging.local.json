{
  "version": 1,
  "formatters": {
    "default": {
      "format": "%(asctime)s - %(levelname)-5s - %(name)-20.20s - %(filename)s:%(lineno)s - %(funcName)s - %(message)s",
      "datefmt": "%m-%d %H:%M:%S"
    }
  },
  "handlers": {
    "root": {
      "class": "logging.StreamHandler",
      "level": "WARN",
      "formatter": "default",
      "stream": "ext://sys.stdout"
    },
    "simple_web_app": {
      "class": "logging.StreamHandler",
      "level": "INFO",
      "formatter": "default",
      "stream": "ext://sys.stdout"
    },
    "uvicorn": {
      "class": "logging.StreamHandler",
      "level": "INFO",
      "formatter": "default",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "simple_web_app": {
      "level": "INFO",
      "handlers": ["simple_web_app"]
    },
    "uvicorn": {
      "level": "INFO",
      "handlers": ["uvicorn"]
    }
  },
  "root": {
    "level": "WARN",
    "handlers": ["root"]
  }
}
